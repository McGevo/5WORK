@model IEnumerable<EATApp.Models.studentsession>

@{
    ViewBag.Title = "Employee Attendance";
}

    <h2>Employee Attendance</h2>

<p>
    @Html.ActionLink("Enter Code", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Sign In
        </th>
        <th>
            Sign Out
        </th>
        <th>
            Date
        </th>

        <th></th>
    </tr>
     
@foreach (var item in Model.Where(s=> s.student_StudentID.Contains("100100100"))) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SignIn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SignOut)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.session.Date)
        </td>
        

    </tr>
}

</table>
